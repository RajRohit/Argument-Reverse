#include<stdio.h>
#include<string.h>

  int main(int arg,char *str[]){
	
  char str2[20];
	int len=0;

	strcpy(str2,str[1]);//Taking Argument
	
	printf("\n%s",str2);
	
	len=strlen(str2);
	
	printf("\n Len: %d \n",len);
	
	str2[++len]='\0';//embedding end with NULL
	
	while(len>0){
	--len;
	printf("%c",str2[len]);
	}
	
	printf("\n");
	return 0;
}
