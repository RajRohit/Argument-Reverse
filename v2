#include<stdio.h>
#include<string.h>

int main(int arg,char *str[]){
	char str2[20][20];
	int len[20]={0};
	printf("Arg: %d ",arg);
	
	for (int i=0;i<arg;++i){
		// Take Argument Input
		strcpy(str2[i],str[i]);
	}
	
	//Accept Length Of Each
	for (int i=0;i<arg;++i)	
		len[i]=strlen(str2[i]);
		
	//Define End Of Each	
	for (int i=0;i<arg;++i){
		++len[i];
		str2[len[i]][i]='\0';
	}
		
	printf("\n");
	
	//Print Reverse		
	for (int i=1;i<(arg);++i){
		while(len[i]>0){
			--len[i];
			printf("%c",str2[i][len[i]]);	
		}
		printf("\n");
	}	

	printf("\n");
	
	return 0;
}
